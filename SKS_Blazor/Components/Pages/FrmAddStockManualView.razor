@using Gap.Blazor;
@using SKS_Blazor.Components;
@inherits ComponentBase
<WMFormComponent model="@this.model">
    <WMLabelComponent model="@this.model.Label3"></WMLabelComponent>
    <WMGroupBoxComponent model="@this.model.Frame1">
        <WMLabelComponent model="@this.model.Label5"></WMLabelComponent>
        <WMTextBoxComponent model="@this.model.txtCode"></WMTextBoxComponent>
        <WMButtonComponent model="@this.model.cmdProducts"></WMButtonComponent>
        <WMLabelComponent model="@this.model.Label4"></WMLabelComponent>
        <WMTextBoxComponent model="@this.model.txtName"></WMTextBoxComponent>
    </WMGroupBoxComponent>
    <WMListView model="@this.model.lvProducts"></WMListView>
    <WMLabelComponent model="@this.model.Label8"></WMLabelComponent>
    <WMTextBoxComponent model="@this.model.txtProductName"></WMTextBoxComponent>
    <WMLabelComponent model="@this.model.Label9"></WMLabelComponent>
    <WMTextBoxComponent model="@this.model.txtUnit"></WMTextBoxComponent>
    <WMLabelComponent model="@this.model.Label2"></WMLabelComponent>  
    <WMTextBoxComponent model="@this.model._txtValues_0"></WMTextBoxComponent>
    <WMLabelComponent model="@this.model.Label7"></WMLabelComponent>
    <WMTextBoxComponent model="@this.model.txtQuantityPerUnit"></WMTextBoxComponent>
    <WMLabelComponent model="@this.model.Label1"></WMLabelComponent>
    <WMTextBoxComponent model="@this.model._txtValues_1"></WMTextBoxComponent>
    <WMLabelComponent model="@this.model.Label6"></WMLabelComponent>
    <WMTextBoxComponent model="@this.model._txtValues_2"></WMTextBoxComponent>
    <WMLabelComponent model="@this.model.Label10"></WMLabelComponent>
    <WMLabelComponent model="@this.model.lblNewQuantity"></WMLabelComponent>
    <WMButtonComponent model="@this.model.cmdSave"></WMButtonComponent>
    <WMButtonComponent model="@this.model.cmdClose"></WMButtonComponent>
    <WMStatusStrip model="@this.model.sbStatusBar"></WMStatusStrip>
</WMFormComponent>

@code {
    [Parameter]
    public frmAddStockManual model { get; set; }

    protected override void OnInitialized()
    {
        base.OnInitialized();
        model = model == null ? new frmAddStockManual() : model;
        model.View = this.GetType();
    }
}