@page "/frmSplash"
@using Gap.Blazor.Components
@rendermode InteractiveServer
@using SKS_Blazor.Components
@inherits ComponentBase
@namespace SKS_Blazor.Components.Pages

<WMFormComponent model="@this.model">
    <WMPictureBoxComponent model="@this.model.Image1"></WMPictureBoxComponent>
</WMFormComponent>

@code {
    [Parameter]
    public frmSplash model { get; set; }

    protected override void OnInitialized()
    {
        base.OnInitialized();
        model = model == null ? new frmSplash() : model;
        model.View = this.GetType();
    }
}
